[extensions]
pager=
mq=
hgext.forest=/Library/Python/2.6/site-packages/forest.py
color =
hgext.crecord = ~/.hgext/crecord
hgext.hgshelve = ~/.hgext/hgshelve/hgshelve.py
progress =
[pager]
pager = LESS='FSRX' less
[color]
qseries.applied = blue bold underline
qseries.unapplied = black bold
qseries.missing = red bold
status.modified = blue bold underline red_background
status.added = green bold
status.removed = red bold blue_background
status.deleted = cyan bold underline
status.unknown = magenta bold underline
status.ignored = black bold
diff.diffline = black bold
diff.extended = cyan bold
diff.file_a = red bold
diff.file_b = green bold
diff.hunk = magenta
diff.deleted = red
diff.inserted = green
diff.changed = bold black
diff.trailingwhitespace = bold red_background
[ui]
username = Daemian Mack <dmack@rpath.com>
[alias]
blame = annotate --date -q --user --number
[diff]
showfunc=1
unified=8
